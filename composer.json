    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/r0m1g0/yii2-treegrid/issues",
        "source": "https://github.com/r0m1g0/yii2-treegrid"
    },
    "authors": [
        {
            "name": "Leandro Guindani Gehlen",
            "email": "leandrogehlen@gmail.com"
        },
        {
            "name": "Roman Kozhushko",
            "email": "roman@kozhushko.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13",
        "bower-asset/jquery-treegrid": "0.3.0",
        "bower-asset/jquery-cookie": ">= 1.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": { "r0m1g0\\treegrid\\": "" }
    }
